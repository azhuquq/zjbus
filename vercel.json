{
    "rewrites": [
      {
        "source": "/wechatapi/:path*",
        "destination": "http://wechat.zhjgongjiao.com/:path*"
      },
      {
        "source": "/websiteapi/:path*",
        "destination": "http://zhjgongjiao.com/:path*"
      }
    ],
    "headers": [
      {
        "source": "/websiteapi/:path*",
        "headers": [
          {
            "key": "Host",
            "value": "zhjgongjiao.com"
          }
        ]
      }
    ]
  }
  